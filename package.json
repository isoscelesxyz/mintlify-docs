{
  "name": "aerodrome-docs",
  "version": "1.0.0",
  "description": "Official documentation for Aerodrome Finance - the essential trading hub and liquidity marketplace on Base",
  "private": true,
  "scripts": {
    "dev": "mint dev --port 8453",
    "clear": "mint clear",
    "format": "prettier --write \"**/*.{mdx,md,json}\"",
    "format:check": "prettier --check \"**/*.{mdx,md,json}\"",
    "format:mdx": "prettier --write \"**/*.mdx\"",
    "format:json": "prettier --write \"**/*.json\"",
    "lint": "pnpm lint:js && pnpm lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2",
    "lint:mdx": "markdownlint-cli2 \"**/*.mdx\"",
    "lint:fix": "markdownlint-cli2 --fix",
    "validate": "pnpm format:check && pnpm lint"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "markdownlint-cli2": "^0.19.0",
    "markdownlint-cli2-formatter-pretty": "^0.0.9",
    "mint": "latest",
    "prettier": "^3.6.2"
  },
  "keywords": [
    "aerodrome",
    "defi",
    "documentation",
    "mintlify"
  ],
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}
