{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Aerodrome Documentation",
  "colors": {
    "primary": "#0052FF",
    "light": "#4A90FF",
    "dark": "#0041CC"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "favicon": "/favicon.ico",
  "metadata": {
    "description": "Official documentation for Aerodrome Finance - the essential trading hub and liquidity marketplace on Base",
    "og:site_name": "Aerodrome Documentation",
    "og:title": "Aerodrome Finance Documentation",
    "og:description": "Learn how to swap, provide liquidity, lock AERO, and vote on emissions on Aerodrome Finance",
    "twitter:site": "@AerodromeFi"
  },
  "search": {
    "placeholder": "Search Aerodrome docs..."
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Overview",
        "groups": [
          {
            "group": "Introduction",
            "icon": "book",
            "pages": ["overview/index"]
          },
          {
            "group": "Getting Started",
            "icon": "rocket",
            "pages": [
              "overview/getting-started/before-you-begin",
              "overview/getting-started/aerodrome-101",
              "overview/getting-started/key-concepts",
              "overview/getting-started/quickstart"
            ]
          },
          {
            "group": "For Users",
            "icon": "user",
            "pages": [
              "overview/core-features/swap",
              {
                "group": "Liquidity",
                "pages": [
                  "overview/core-features/liquidity-pools/index",
                  "overview/core-features/liquidity-pools/providing-liquidity",
                  "overview/core-features/liquidity-pools/staking",
                  "overview/core-features/liquidity-pools/basic-pools",
                  "overview/core-features/liquidity-pools/concentrated-pools",
                  "overview/core-features/liquidity-pools/autopilot"
                ]
              },
              {
                "group": "Lock",
                "pages": ["overview/core-features/lock/index", "overview/core-features/lock/relay"]
              },
              "overview/core-features/vote",
              "overview/core-features/rewards",
              "overview/getting-started/flight-school"
            ]
          },
          {
            "group": "For Protocols",
            "icon": "users",
            "pages": [
              "overview/partners/index",
              "overview/partners/aero-ignition",
              "overview/partners/launcher",
              "overview/partners/incentives"
            ]
          },
          {
            "group": "Tokenomics",
            "icon": "coins",
            "pages": ["overview/tokenomics/overview"]
          },
          {
            "group": "Security",
            "icon": "shield",
            "pages": ["overview/security/overview", "overview/security/audits"]
          },
          {
            "group": "Contributors",
            "icon": "handshake",
            "pages": [
              "overview/contributors/dromos-labs",
              "overview/contributors/aerodrome-foundation"
            ]
          }
        ]
      },
      {
        "tab": "Development",
        "groups": [
          {
            "group": "Developer Resources",
            "icon": "code",
            "pages": [
              "development/sugar",
              "development/data-analytics",
              "development/third-party-integrations"
            ]
          }
        ]
      },
      {
        "tab": "Help",
        "groups": [
          {
            "group": "Support",
            "icon": "life-ring",
            "pages": ["help/discord"]
          },
          {
            "group": "FAQ",
            "icon": "circle-question",
            "pages": [
              "help/faqs/users",
              {
                "group": "For Partners",
                "pages": ["help/faqs/partners/partners", "help/faqs/partners/stablecoin-partners"]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "aerodrome.finance",
          "href": "https://aerodrome.finance",
          "icon": "external-link"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Community",
        "href": "https://discord.gg/aerodrome"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://aerodrome.finance"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "styling": {
    "latex": true
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/AerodromeFi",
      "github": "https://github.com/aerodrome-finance",
      "discord": "https://discord.gg/aerodrome",
      "medium": "https://medium.com/@AerodromeFi",
      "website": "https://aerodrome.finance"
    }
  }
}
